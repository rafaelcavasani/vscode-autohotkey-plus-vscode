{
	"name": "vscode-autohotkey-plus-slimcode",
	"displayName": "AutoHotKey SlimCode",
	"description": "AutoHotkey language support for VS Code",
	"version": "2.1.16",
	"publisher": "rafaelcavasani",
	"engines": {
		"vscode": "^1.30.0"
	},
	"activationEvents": [
		"*"
	],
	"main": "./out/extension",
	"categories": [
		"Programming Languages",
		"Snippets",
		"Formatters"
	],
	"license": "SEE LICENSE IN LICENSE.md",
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://rafaelcavasani.visualstudio.com/vscode-autohotkey-plus-slimcode/_git/vscode-autohotkey-plus-slimcode"
	},
	"galleryBanner": {
		"color": "#5c2d91",
		"theme": "dark"
	},
	"keywords": [
		"vscode",
		"autohotkey",
		"ahk"
	],
	"contributes": {
		"languages": [
			{
				"id": "ahk",
				"aliases": [
					"AutoHotkey",
					"autohotkey",
					"ahk"
				],
				"extensions": [
					".ahk",
					".ext"
				],
				"configuration": "./ahk.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "ahk",
				"scopeName": "source.ahk",
				"path": "./syntaxes/ahk.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "ahk",
				"path": "./snippets/ahk.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"tslint": "tslint -t verbose src/**/*.ts"
	},
	"devDependencies": {
		"@types/node": "^8.10.25",
		"tslint": "^5.15.0",
		"typescript": "^3.4.3",
		"vscode": "^1.1.36"
	},
	"dependencies": {
		"babel": "^6.23.0",
		"highlight": "^0.2.4",
		"svn": "^0.3.0",
		"vscode-test": "^1.2.0"
	}
}
